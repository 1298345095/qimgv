[accessibleName="panelButton"] {
    background-color: #1f222a;
    padding: 3px 3px 3px 3px;
}

[accessibleName="panelButton"]:hover {
    background-color: #404552;
}

[accessibleName="panelButtonTransparent"] {
    padding: 3px 3px 3px 3px;
}

[accessibleName="panelButtonTransparent"]:hover {
    background-color: #2b2e39;
}

[accessibleName="panelButtonsWidget"] {
    background-color: #1f222a;
}

[accessibleName="thumbnailWidget"] {
    background-color: #1f222a;
}

[accessibleName="thumbnailView"] {
    background-color: #1f222a;
}

ThumbnailLabel {
    background-color: #1f222a;
}

QScrollBar:horizontal {
    height: 15px;
    background-color: #1f222a;
}

QScrollBar::handle:horizontal {
    background: #565d6e;
    min-width: 60px;
}

QScrollBar::handle:horizontal:hover {
    background: #656e82;
}

QGraphicsView {
    background: #383c4a;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}

QScrollBar::add-line:horizontal {
    width: 0px;
    height: 0px;
}

QScrollBar::sub-line:horizontal {
    width: 0px;
    height: 0px;
}

QComboBox {
    padding-left: 4px;
    padding-right: 3px;
}

QGraphicsView {
    border-style: none;
}
